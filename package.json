{
  "name": "@amiearth/thai-address-finder",
  "version": "0.0.16",
  "description": "A library to search and find Thai address information",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "build:data": "node scripts/minify-addresses.js",
    "serve:data": "node scripts/serve-data-server.js",
    "pack:dry-run": "npm pack --dry-run",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src tests --ext .ts",
    "lint:fix": "eslint src tests --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "prepublishOnly": "npm run build:data && npm run build",
    "publish:patch": "npm version patch && npm publish --access public",
    "publish:minor": "npm version minor && npm publish --access public",
    "publish:major": "npm version major && npm publish --access public"
  },
  "keywords": [
    "thai",
    "address",
    "thailand",
    "postal-code",
    "province",
    "district",
    "sub-district",
    "autocomplete"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/earth774/thai-address-finder.git"
  },
  "bugs": {
    "url": "https://github.com/earth774/thai-address-finder/issues"
  },
  "homepage": "https://earth774.github.io/thai-address-finder-demo/",
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/data/geography.json"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.2.4",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.3",
    "compression": "^1.7.4",
    "express": "^4.18.2"
  }
}
